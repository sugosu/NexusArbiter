{
  "log_io": {
    "enabled": true,
    "log_dir": "logs/library_manager/io",
    "request_file_pattern": "{run_name}__{attempt}__request.json",
    "response_file_pattern": "{run_name}__{attempt}__response.json"
  },

  "runs": [
    {
      "name": "library_manager_generate_app",
      "profile_file": "context_files/profiles/python/openai-5/coders/dummy.json",
      "provider": "openai",
      "task_description": "Generate a minimal Python implementation for class library.app.App.",
      "target_file": "library_manager/library/app.py",
      "rerun_strategy": null,
      "rerun_index": 0,
      "target_run": null
    },

    {
      "name": "library_manager_generate_controller",
      "profile_file": "context_files/profiles/python/openai-5/coders/dummy.json",
      "provider": "openai",
      "task_description": "Generate a minimal Python implementation for class library.controller.LibraryController.",
      "target_file": "library_manager/library/controller.py",
      "rerun_strategy": null,
      "rerun_index": 0,
      "target_run": null
    },

    {
      "include_run": "library_manager/runs/domain_and_infra.json"
    }
  ]
}
