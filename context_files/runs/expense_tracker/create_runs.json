{
  "log_io": {
    "enabled": true,
    "log_dir": "logs/io",
    "request_file_pattern": "{run_name}__{attempt}__request.json",
    "response_file_pattern": "{run_name}__{attempt}__response.json"
  },
  "runs": [
    {
      "name": "run_from_manifest_generator",
